    "@fortawesome/react-fontawesome": "^0.1.11",
    "@hookform/resolvers": "^0.1.0",
    "autoprefixer": "^9.8.5",
    "axios": "^0.19.2",
    "axios-case-converter": "^0.6.0",
    "flat": "^5.0.0",
    "immer": "^7.0.7",
    "md5": "^2.2.1",
    "node-sass": "^4.14.1",
    "postcss-cli": "^7.1.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.1.2",
    "react-intl": "^5.4.2",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^6.0.0",
    "redux-promise-middleware": "^6.1.2",
    "redux-thunk": "^2.3.0",
    "yup": "^0.29.1"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "test:coverage": "react-scripts test --env=jest-environment-jsdom-sixteen --coverage --watchAll=false",
    "lint": "eslint .",
    "eject": "react-scripts eject",
    "build:styles": "postcss src/tailwind.base.css -o src/tailwind.css",
    "prebuild": "yarn build:styles",
    "prestart": "yarn build:styles",
    "pre-push": "eslint ."
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^10.4.7",
    "decamelize": "^4.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest-dom": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.4",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-testing-library": "^3.3.2",
    "history": "^5.0.0",
    "husky": "^4.2.5",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "map-obj": "^4.1.0",
    "nock": "^13.0.2",
    "prettier": "^2.0.5",
    "react-axe": "^3.5.3",
    "source-map-explorer": "^2.4.2",
    "tailwindcss": "^1.4.6"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn pre-push"
    }
  }
}
