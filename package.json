    "flat": "^5.0.0",
    "humps": "^2.0.1",
    "immer": "^6.0.9",
    "node-sass": "^4.14.1",
    "postcss-cli": "^7.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^5.7.2",
